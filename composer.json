{
    "name": "shakewellagency/litecard-laravel",
    "description": "A comprehensive Laravel package for LiteCard digital membership card integration.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["laravel", "litecard", "digital-cards", "membership", "api"],
    "autoload": {
        "psr-4": {
            "Shakewell\\Litecard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shakewell Agency",
            "email": "info@shakewellagency.com"
        },
        {
            "name": "Ehsan Tavakoli",
            "email": "ehsantavakoli.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/log": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8|^8.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shakewell\\Litecard\\LiteCardServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shakewell\\Litecard\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/pint"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
