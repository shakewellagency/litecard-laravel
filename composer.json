{
    "name": "shakewell/litecard",
    "description": "A Laravel package for Litecard integration.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shakewell\\Litecard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ehsan Tavakoli",
            "email": "ehsantavakoli.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shakewell\\Litecard\\LitecardServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shakewell\\Litecard\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
