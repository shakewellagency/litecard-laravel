{
    "name": "shakewellagency/litecard-laravel",
    "description": "A comprehensive Laravel package for LiteCard digital membership card integration.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["laravel", "litecard", "digital-cards", "membership", "api"],
    "autoload": {
        "psr-4": {
            "Shakewell\\Litecard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shakewell Agency",
            "email": "info@shakewellagency.com"
        },
        {
            "name": "Ehsan Tavakoli",
            "email": "ehsantavakoli.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/log": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shakewell\\Litecard\\LiteCardServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shakewell\\Litecard\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
